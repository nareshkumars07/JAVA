//NEGATIVE OR POSITIVE
import java.util.Scanner;
public class main{
    public static void main(String[] args){
     Scanner sc = new Scanner(System.in);
     System.out.println("value  :");
     int a = sc.nextInt();
     if(a>0){
     System.out.println("positive");
     }else{
         System.out.println("negative");
         
     }
   
    }
}
